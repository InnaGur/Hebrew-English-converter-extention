{
    "manifest_version": 3,
    "name": "Hebrew-English Text Converter",
    "version": "1.0",
    "permissions": [
      "contextMenus",
      "activeTab",
      "scripting"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icons/tmp.png",
        "48": "icons/tmp.png",
        "128": "icons/tmp.png"
      }
    }
  }
  